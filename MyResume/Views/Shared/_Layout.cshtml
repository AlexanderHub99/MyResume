<body id="particlesJS">
    <div class="container_My_resume ">
        <div class="wrapper">
            <div>
                <div class="box_img_container_top ">
                    <div class="box_img">
                        <img class="author__image" src="~/img/631978956.jpeg" alt="Telegram"></img>
                        <div class="box_img_container">
                            <div class="box_img_text">
                                <h1>My Telegram</h1>
                                <p>IscanderDanielovich</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div>
                    <h1>Обо мне:</h1>
